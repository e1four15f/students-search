<Window x:Class="GUI.ListOperation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="NoResize"
        Title="ListOperation" Height="150" Width="300"
        Icon="/GUI;component/resources/img/miet_logo.png">
	
    <Grid>
        <StackPanel Margin="10">
            <Button Margin="5" Width="150" Content="Найти email-ы" Click="ButtonFindEmails"/>
            <Button Margin="5" Width="200" Content="Найти на популярных сайтах" Click="ButtonMostLikelySites"/>
            <Button Margin="5" Width="200" Content="Найти в поисковиках" Click="ButtonGoogleIt"/>
        </StackPanel>
    </Grid>
</Window>
